publishStateDiv = document.getElementById("check-publish-data")

<% if @status %>
  publishStateDiv.innerHTML = "<strong> Publishable </strong>"
  publishStateDiv.className = "bg-success text-center border"
<% else %>
  publishStateDiv.innerHTML = "<strong> NOT Publishable </strong >"
  publishStateDiv.className = "bg-danger text-center border"
<%end%>
